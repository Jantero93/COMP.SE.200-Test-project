{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-console": "warn",
    "eqeqeq": "warn",
    "no-unused-vars": "error"
  }
}
